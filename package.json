    "@ngrx/effects": "^8.0.0-rc.1",
    "@ngrx/entity": "^8.0.0-rc.1",
    "@ngrx/router-store": "^8.0.0-rc.1",
    "@ngrx/store": "^8.0.0-rc.1",
    "@ngrx/store-devtools": "^8.0.0-rc.1",
